<%
locals.OLSKPageAssets = [
	'/_shared/__external/normalize.css/normalize.css',
	'__compiled/ui-style.css',
];
%>

<script src="/<%= OLSKSharedPageControllerSlug %>/__compiled/ui-behaviour.js"></script>

<hr role="presentation" />

<p>
	<strong>ResultsHidden</strong>
	<input type="checkbox" oninput="LCHLauncherPrompt.$set({ResultsHidden: this.checked})" id="LCHLauncherPromptTestSetResultsHidden" />
</p>

<p>
	<strong>PromptItems</strong>
	<button onclick="LCHLauncherPrompt.$set({StubItems: []})" id="LCHLauncherPromptTestSetPromptItemsZero">Zero</button>
	<button onclick="LCHLauncherPrompt.$set({StubItems: [{
		LCHRecipeName: 'alfa',
	}]})" id="LCHLauncherPromptTestSetPromptItemsSingle">Single</button>
	<button onclick="LCHLauncherPrompt.$set({StubItems: [{
		LCHRecipeName: 'alfa',
	},{
		LCHRecipeName: 'bravo',
	},{
		LCHRecipeName: 'charlie',
	}]})" id="LCHLauncherPromptTestSetPromptItemsMultiple">Multiple</button>
</p>
